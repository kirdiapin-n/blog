{
  "name": "blog",
  "version": "1.0.0",
  "scripts": {
    "dev": "firebase emulators:exec \"next dev\"",
    "dev:external": "sh -c 'export IP=$(ipconfig getifaddr en0); next dev -H $IP -p 4283'",
    "start": "next dev",
    "firebase:emulators": "firebase emulators:start --only auth,firestore,storage",
    "build": "next build && next export -o build/",
    "lint": "eslint \"./**/*.{ts, tsx}\"",
    "prettier": "prettier --check \"./**/*.{ts,tsx}\"",
    "test": "npm run lint && npm run prettier && jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "keywords": [
    "boilerplate",
    "react",
    "next"
  ],
  "author": "nikita.kirdiapin@gmail.com",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@uiw/react-md-editor": "^3.20.2",
    "classnames": "^2.3.1",
    "effector": "^22.5.0",
    "effector-react": "^22.4.0",
    "firebase": "^9.6.10",
    "fp-ts": "^2.11.9",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jest": "^27.5.1",
    "moment": "^2.29.3",
    "next": "^13.1.4",
    "next-remove-imports": "^1.0.8",
    "next-themes": "^0.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "babel-jest": "^28.1.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-effector": "^0.10.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "2.5.1",
    "typescript": "^4.6.2"
  }
}
